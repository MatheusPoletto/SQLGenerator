<sgbd type="SQLServer">
	<commands>
		<command type="select">
			<select>SELECT</select>	
			<distinct>DISTINCT</distinct>			
			<all>*</all>
			<column>${V1} AS ${V2}</column>
			<from>FROM ${V1}</from>
			<join>INNER JOIN ${V1} ON 0 = 0 <and>AND ${V1} = ${V2}</and></join>
			<where>WHERE ${V1} = ${V2}</where>
			<and>AND ${V1} = ${V2}</and>
			<orderby>ORDER BY 
				<asc>${V1} ASC</asc>
				<desc>${V1} DESC</desc>
			</orderby>
			<groupby>GROUP BY <item>${V1}</item></groupby>
			<top>LIMIT ${V1}</top>
		</command>
		<command type="insert">
			<insert>INSERT</insert>
			<into>INTO ${V1} (</into>
			<column>${V1}</column>
			<values>) VALUES (</values>
			<value>${V1}</value>
			<end>)</end>
		</command>
		<command type="update">
			<update>UPDATE</update>
			<table>${V1}</table>
			<set>SET</set>
			<column>${V1} = ${V2}</column>
			<where>WHERE ${V1} = ${V2}</where>
			<and>AND ${V1} = ${V2}</and>
		</command>
		<command type="delete">
			<delete>DELETE</delete>
			<from>FROM ${V1}</from>
			<where>WHERE ${V1} = ${V2}</where>
			<and>AND ${V1} = ${V2}</and>
		</command>
		
	</commands>
</sgbd>